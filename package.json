        "@types/lodash": "^4.14.118",
        "@types/node": "^10.12.9",
        "@types/pino": "^5.8.1",
        "@types/request": "^2.48.1",
        "@types/request-promise-native": "^1.0.15",
        "jest": "^23.6.0",
        "prettier": "^1.15.2",
        "ts-jest": "^23.10.4",
        "ts-loader": "^5.3.0",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.7.0",
